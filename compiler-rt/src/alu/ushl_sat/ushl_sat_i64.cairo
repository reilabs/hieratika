use crate::rtstate::RTState;
use crate::alu::ushl_sat::ushl_sat;

pub fn __llvm_ushl_sat_l_l_l(ref state: RTState, n: u128, shift: u128) -> u128 {
    ushl_sat::<u64>(n, shift)
}

